                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Source" TargetName="aaa" Value="/KanDic;component/Cache/icon/expand/2.PNG"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter Property="Source" TargetName="aaa" Value="/KanDic;component/Cache/icon/expand/2.PNG"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
